GtkWidget*
create_UnlinkCameraQu (void)
{
  GtkWidget *UnlinkCameraQu;
  GtkWidget *dialog_vbox2;
  GtkWidget *label5;
  GtkWidget *dialog_action_area2;
  GtkWidget *alignment1;
  GtkWidget *ExitUnlinkYes;
  GtkWidget *ExitUnlinkNo;
  GtkWidget *ExitUnlinkCancel;
  GtkTooltips *tooltips;

  tooltips = gtk_tooltips_new ();

  UnlinkCameraQu = gtk_dialog_new ();
  gtk_window_set_title (GTK_WINDOW (UnlinkCameraQu), _("Exit (unlink)"));

  dialog_vbox2 = GTK_DIALOG (UnlinkCameraQu)->vbox;
  gtk_widget_show (dialog_vbox2);

  label5 = gtk_label_new (_("Camera still linked.\nWant to unlink it?"));
  gtk_widget_show (label5);
  gtk_box_pack_start (GTK_BOX (dialog_vbox2), label5, FALSE, FALSE, 0);

  dialog_action_area2 = GTK_DIALOG (UnlinkCameraQu)->action_area;
  gtk_widget_show (dialog_action_area2);
  gtk_button_box_set_layout (GTK_BUTTON_BOX (dialog_action_area2), GTK_BUTTONBOX_END);

  alignment1 = gtk_alignment_new (0.5, 0.5, 1, 1);
  gtk_widget_show (alignment1);
  gtk_container_add (GTK_CONTAINER (dialog_action_area2), alignment1);

  ExitUnlinkYes = gtk_button_new_from_stock ("gtk-yes");
  gtk_widget_show (ExitUnlinkYes);
  gtk_container_add (GTK_CONTAINER (alignment1), ExitUnlinkYes);
  GTK_WIDGET_SET_FLAGS (ExitUnlinkYes, GTK_CAN_DEFAULT);
  gtk_tooltips_set_tip (tooltips, ExitUnlinkYes, _("Click here to unlink before exit"), NULL);

  ExitUnlinkNo = gtk_button_new_from_stock ("gtk-no");
  gtk_widget_show (ExitUnlinkNo);
  gtk_dialog_add_action_widget (GTK_DIALOG (UnlinkCameraQu), ExitUnlinkNo, 0);
  GTK_WIDGET_SET_FLAGS (ExitUnlinkNo, GTK_CAN_DEFAULT);
  gtk_tooltips_set_tip (tooltips, ExitUnlinkNo, _("Click here to stay linked and exit"), NULL);

  ExitUnlinkCancel = gtk_button_new_from_stock ("gtk-cancel");
  gtk_widget_show (ExitUnlinkCancel);
  gtk_dialog_add_action_widget (GTK_DIALOG (UnlinkCameraQu), ExitUnlinkCancel, 0);
  GTK_WIDGET_SET_FLAGS (ExitUnlinkCancel, GTK_CAN_DEFAULT);
  gtk_tooltips_set_tip (tooltips, ExitUnlinkCancel, _("Click here to cancel exiting"), NULL);

  g_signal_connect ((gpointer) ExitUnlinkYes, "clicked",
                    G_CALLBACK (on_unlink_yes_click),
